// ┌──────────────────┐
//═╡ Gold and Potions ╞══════════════════════════════════════════════════════════════════════════════════════════════════
// └──────────────────┘
// Hidden: smaller gold piles - levels 10, 20, 30, 50, 70, 80, 90
ItemDisplay[((GOLD<5000 CLVL>89) OR (GOLD<3000 CLVL>79) OR (GOLD<1800 CLVL>69) OR (GOLD<1200 CLVL>49) OR (GOLD<600 CLVL>29) OR (GOLD<300 CLVL>19) OR (GOLD<100 CLVL>9) OR GOLD<30)]:
ItemDisplay[GOLD>0]: %NAME%

// Hidden: minor, lesser, standard, & greater potions - levels 18, 30, 40, 75
ItemDisplay[(hp1 OR mp1) CLVL>17]:
ItemDisplay[(hp2 OR mp2) CLVL>29]:
ItemDisplay[(hp3 OR mp3) CLVL>39]:
ItemDisplay[(hp4 OR mp4) CLVL>74]:
ItemDisplay[hp1]:	%RED%[] %GRAY%h¹			// Minor Healing
ItemDisplay[mp1]:	%BLUE%[] %GRAY%m¹			// Minor Mana
ItemDisplay[hp2]:	%RED%[] %GRAY%h²			// Lesser Healing
ItemDisplay[mp2]:	%BLUE%[] %GRAY%m²			// Lesser Mana
ItemDisplay[hp3]:	%RED%[] %GRAY%H³			// Healing
ItemDisplay[mp3]:	%BLUE%[] %GRAY%M³			// Mana
ItemDisplay[hp4]:	%RED%[] %WHITE%H-			// Greater Healing
ItemDisplay[mp4]:	%BLUE%[] %WHITE%M-			// Greater Mana

// Hidden: Stamina Potion, Thawing Potion - level 30
ItemDisplay[(wms OR vps) CLVL>29]:
ItemDisplay[vps]:	%WHITE%[] %GRAY%s			// Stamina
ItemDisplay[wms]:	%YELLOW%[] %GRAY%t			// Thawing

// Hidden: Throwing Potions - level 15
ItemDisplay[(opl OR opm OR ops OR gpl OR gpm OR gps) CLVL>14]:
ItemDisplay[opl]:	%DGREEN%[] %GRAY%f¹			// Fulminating
ItemDisplay[opm]:	%DGREEN%[] %GRAY%f²			// Exploding
ItemDisplay[ops]:	%DGREEN%[] %GRAY%f³			// Oil
ItemDisplay[gpl]:	%DGREEN%[] %GRAY%p¹			// Strangling
ItemDisplay[gpm]:	%DGREEN%[] %GRAY%p²			// Choking
ItemDisplay[gps]:	%DGREEN%[] %GRAY%p³			// Rancid

// Hidden: Antidote Potion - 90
ItemDisplay[yps CLVL>89]:

// Other Potions
ItemDisplay[hp5]:	%RED%[] %WHITE%H			// Super Healing
ItemDisplay[mp5]:	%BLUE%[] %WHITE%M			// Super Mana
ItemDisplay[rvs]:	%PURPLE%[] %WHITE%r-			// Rejuvenation
ItemDisplay[rvl]:	%PURPLE%[] %WHITE%R70			// Full Rejuvenation
ItemDisplay[yps]:	%DGREEN%[] %GRAY%A			// Antidote


